{
    "tech_stack": [
        "TypeScript",
        "Next.js 16.1.3",
        "React 19",
        "LangChain",
        "LangGraph",
        "Supabase (PostgreSQL)",
        "Pinecone (Vector DB)",
        "Tailwind CSS 4"
    ],
    "features": [
        {
            "name": "Agent Run API",
            "description": "Start the AI agent to process news articles and generate social media posts",
            "files": [
                "app/api/agent/run/route.ts"
            ],
            "api_doc": {
                "openapi": "3.0.0",
                "info": {
                    "title": "Agent Run API",
                    "version": "1.0.0"
                },
                "paths": {
                    "/api/agent/run": {
                        "post": {
                            "summary": "Start agent processing",
                            "responses": {
                                "200": {
                                    "description": "Agent run started successfully",
                                    "content": {
                                        "application/json": {
                                            "schema": {
                                                "type": "object",
                                                "properties": {
                                                    "success": {
                                                        "type": "boolean"
                                                    },
                                                    "runId": {
                                                        "type": "string"
                                                    }
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        {
            "name": "Agent Status API",
            "description": "Get the current status of an agent run including progress and activity logs",
            "files": [
                "app/api/agent/status/route.ts"
            ],
            "api_doc": {
                "openapi": "3.0.0",
                "paths": {
                    "/api/agent/status": {
                        "get": {
                            "summary": "Get agent run status",
                            "responses": {
                                "200": {
                                    "description": "Current agent status",
                                    "content": {
                                        "application/json": {
                                            "schema": {
                                                "type": "object",
                                                "properties": {
                                                    "isRunning": {
                                                        "type": "boolean"
                                                    },
                                                    "currentRun": {
                                                        "type": "object"
                                                    },
                                                    "activity": {
                                                        "type": "array"
                                                    }
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        {
            "name": "Agent Queue API",
            "description": "Get queue items for a specific agent run",
            "files": [
                "app/api/agent/queue/[runId]/route.ts"
            ],
            "api_doc": {
                "openapi": "3.0.0",
                "paths": {
                    "/api/agent/queue/{runId}": {
                        "get": {
                            "summary": "Get queue items for a run",
                            "parameters": [
                                {
                                    "name": "runId",
                                    "in": "path",
                                    "required": true,
                                    "schema": {
                                        "type": "string"
                                    }
                                }
                            ],
                            "responses": {
                                "200": {
                                    "description": "Queue items for the run"
                                }
                            }
                        }
                    }
                }
            }
        },
        {
            "name": "Feeder Sources API",
            "description": "Manage RSS feed sources - create, read, update, delete",
            "files": [
                "app/api/feeder/sources/route.ts"
            ],
            "api_doc": {
                "openapi": "3.0.0",
                "paths": {
                    "/api/feeder/sources": {
                        "get": {
                            "summary": "Get all RSS sources",
                            "responses": {
                                "200": {
                                    "description": "List of RSS sources"
                                }
                            }
                        },
                        "post": {
                            "summary": "Create new RSS source",
                            "requestBody": {
                                "content": {
                                    "application/json": {
                                        "schema": {
                                            "type": "object",
                                            "properties": {
                                                "name": {
                                                    "type": "string"
                                                },
                                                "feed_url": {
                                                    "type": "string"
                                                }
                                            }
                                        }
                                    }
                                }
                            },
                            "responses": {
                                "200": {
                                    "description": "Source created"
                                }
                            }
                        }
                    }
                }
            }
        },
        {
            "name": "Feeder Settings API",
            "description": "Get and update news feeder settings",
            "files": [
                "app/api/feeder/settings/route.ts"
            ],
            "api_doc": {
                "openapi": "3.0.0",
                "paths": {
                    "/api/feeder/settings": {
                        "get": {
                            "summary": "Get feeder settings",
                            "responses": {
                                "200": {
                                    "description": "Current feeder settings"
                                }
                            }
                        },
                        "patch": {
                            "summary": "Update feeder settings",
                            "responses": {
                                "200": {
                                    "description": "Settings updated"
                                }
                            }
                        }
                    }
                }
            }
        },
        {
            "name": "Posts API",
            "description": "Get generated social media posts",
            "files": [
                "app/api/posts/route.ts"
            ],
            "api_doc": {
                "openapi": "3.0.0",
                "paths": {
                    "/api/posts": {
                        "get": {
                            "summary": "Get generated posts",
                            "responses": {
                                "200": {
                                    "description": "List of generated posts"
                                }
                            }
                        }
                    }
                }
            }
        },
        {
            "name": "API Test Endpoint",
            "description": "Test individual external APIs (Serper, SearXNG, Jina AI)",
            "files": [
                "app/api/test/route.ts"
            ],
            "api_doc": {
                "openapi": "3.0.0",
                "paths": {
                    "/api/test": {
                        "get": {
                            "summary": "Get environment variable status",
                            "responses": {
                                "200": {
                                    "description": "Environment variable status"
                                }
                            }
                        },
                        "post": {
                            "summary": "Test individual APIs",
                            "requestBody": {
                                "content": {
                                    "application/json": {
                                        "schema": {
                                            "type": "object",
                                            "properties": {
                                                "api": {
                                                    "type": "string",
                                                    "enum": [
                                                        "serper_search",
                                                        "serper_news",
                                                        "searxng_search",
                                                        "searxng_news",
                                                        "jina_free",
                                                        "jina_api"
                                                    ]
                                                },
                                                "query": {
                                                    "type": "string"
                                                },
                                                "url": {
                                                    "type": "string"
                                                }
                                            }
                                        }
                                    }
                                }
                            },
                            "responses": {
                                "200": {
                                    "description": "API test result"
                                }
                            }
                        }
                    }
                }
            }
        }
    ]
}